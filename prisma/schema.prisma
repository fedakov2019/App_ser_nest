generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}
model UsersP1 {
  id Int @id @default(autoincrement())
  Login String  @unique
  Password String 
  Acces Boolean 
  RefrechToken String? 
}